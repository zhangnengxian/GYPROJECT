</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[reportcon]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT
	c.proj_no 工程编号,
	c.proj_name 工程名称,
	c.proj_addr 工程地址,
	c.SECOND_PARTY_NAME 客户名称,
	p.contribution_mode_des 出资方式,
	p.project_type_des 工程类型,
  c.SECOND_PARTY_AGENT 客户联系人,
  c.SECOND_PARTY_TEL_NUMBER 客户联系人电话,
	p.PROJ_SCALE_DES 工程规模,
  c.INSTALL_NUMS 户数,
	c.UNIT_COST 每户金额,
	c.TOTAL_COST 合同价,
	c.IMC_NO 合同编号,
	STR_TO_DATE(c.IMC_SIGN_DATE, '%Y-%m-%d') 签订日期,
	STR_TO_DATE(c.predict_complete_date, '%Y-%m-%d') 预计完工日期,
     c.FIRST_PARTY_CON_AGENT 经办人,
     p.SURVEYER 踏勘人,
	s.MOBILE 联系电话
FROM
	intelligent_meter_contract c
LEFT JOIN project p ON c.PROJ_ID = p.PROJ_ID LEFT JOIN staff s on p.SURVEYER_ID=s.STAFF_ID 
WHERE
	1 = 1 ${condition}]]></Query>
</TableData>
