<TableDataMap>
<TableData name="ds1" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="projId"/>
<O>
<![CDATA[]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[reportcon]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT
	a.CON_NO '合同编号',
	a.PROJ_NAME '项目名称',
	a.PROJ_ADDR '项目地址',
	a.CUST_NAME '客户名称',
	a.GAS_COMP '燃气公司',
	a.CONTRACT_AMOUNT '合同价',
	a.CUST_ENTRUST_REPRESENT '客户委托代表',
	a.CON_AGENT '燃气公司方经办人',
	a.INCREMENT '税率',
	a.CONTRACT_COPIES '合同份数',
	a.FISRT_PARTY_COPIES '燃气公司方份数',
	a.SECOND_PARTY_COPIES '客户方份数',
	a.SIGN_DATE '签订日期',
	a.HOSE_AMOUNT '每户金额',
	a.HOUSEHOLD '户数',
	a.OPEN_BANK '开户行',
	a.ACCOUNT '账号',
	a.GAS_CORP_ADDR '燃气公司地址',
	a.HOUSE_DATE '交房日期',
	a.SUPPLEMENT_CLAUSE '补充条款',
	a.CUST_PHONE '客户联系电话',
	a.GAS_COMP_PHONE '燃气公司联系电话',
	a.UNIT_ADDRESS '建设单位地址',
	a.CONTRACT_AMOUNT '共计金额',
	a.PAYMENT_RATIO1 '首付比例',
	a.FIRST_PAYMENT '首付款',
	a.BUDGET_COST '预算总造价',
	a.PROJ_SCALE_DES '工程规模概述',
	a.REMARK '备注',
	a.CONTRACT_CONTENT '合同内容(用气设备)',
	a.TIME_LIMIT '工期'
FROM
	contract a 
WHERE
	a.CON_ID = '${conId}']]></Query>
</TableData>
<TableData name="不含税价" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="conId"/>
<O>
<![CDATA[]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[reportcon]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT
	B.CONTRACT_AMOUNT - B.INCREMENT_AMOUNT '不含税价' 
FROM
	(SELECT
	       A.CONTRACT_AMOUNT,
            CASE
	           WHEN A.INCREMENT_AMOUNT IS NULL THEN
	              0 
	           ELSE A.INCREMENT_AMOUNT 
        	   END INCREMENT_AMOUNT 
      FROM CONTRACT A 
      WHERE A.CON_ID = '${CONID}' 
) B;]]></Query>
</TableData>
<TableData name="支付剩余的金额" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="CONID"/>
<O>
<![CDATA[]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[reportcon]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT
	B.CONTRACT_AMOUNT - B.FIRST_PAYMENT '支付剩余的金额' 
FROM
	(SELECT
	       A.CONTRACT_AMOUNT,
            CASE
	           WHEN A.FIRST_PAYMENT IS NULL THEN
	              0 
	           ELSE A.FIRST_PAYMENT 
        	   END FIRST_PAYMENT 
      FROM CONTRACT A 
      WHERE A.CON_ID = '${CONID}' 
) B;
]]></Query>
</TableData>
<TableData name="签字" class="com.fr.data.impl.DBTableData">
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[reportcon]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT
	a.FIELD_NAME,
	a.IMG_URL,
	a.SIGN_TIME 
FROM
	signature a 
WHERE
a.BUSINESS_ORDER_ID = 'conId';]]></Query>
</TableData>
<TableData name="审核意见" class="com.fr.data.impl.DBTableData">
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[reportcon]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT
	a.MR_AOPINION '审核意见',
	a.MR_AUDIT_LEVEL,
	a.MR_TIME '审核时间'
FROM
	manage_record a
WHERE
	a.BUSINESS_ORDER_ID = 'conId' 
	AND a.step_id = '1403' 
	AND a.FLAG IS NULL]]></Query>
</TableData>
<TableData name="合同类型" class="com.fr.data.impl.DBTableData">
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[reportcon]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT
	CASE
		WHEN a.CONTRACT_TYPE = 11 THEN '居民' 
		WHEN a.CONTRACT_TYPE = 21 THEN '公建' 
		WHEN a.CONTRACT_TYPE = 31 THEN '改管' 
		ELSE '其他' 
	END '合同类型' 
FROM
	contract a 
WHERE
a.CON_ID='{conId}']]></Query>
</TableData>
